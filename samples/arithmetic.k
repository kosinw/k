import std

fn main(): void {
    let fifteen = 15

    let sum: i32 = 20 + fifteen
    let difference = -20 - fifteen
    let product = fifteen * 88
    let quotient = fifteen / 2
    let integerQuotient = fifteen

    std.println(sum)
    std.println(difference)
    std.println(product)
    std.println(quotient)
    std.println(integerQuotient)
}